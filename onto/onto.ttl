@prefix football: <http://omeka.org/s/vocabs/football#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# football

football:Personne a rdfs:Class ;
    rdfs:label "Personne" .

football:Equipe a rdfs:Class ;
    rdfs:label "Equipe" .

football:Joueur a rdfs:Class ;
    rdfs:label "Joueur" ;
    rdfs:subClassOf football:Personne .

football:IsPlayerOf a rdf:Property ;
    rdfs:label "Joue pour une équipe" ;
    rdfs:domain football:Joueur .

football:IsPartOf a rdf:Property ;
    rdfs:label "fait partie d’une team" ;
    rdfs:domain football:Joueur .

football:HasNumber rdf:Property ;
    rdfs:label "joueur possédant un numéro de tshirt" ;
    rdfs:domain football:Joueur .

football:HasPosition rdf:Property ;
    rdfs:label "joueur a un poste sur le terrain" ;
    rdfs:domain football:Joueur .

football:IsComposed rdf:Property ;
    rdfs:label "équipe composé de joueurs" ;
    rdfs:domain football:Equipe ;
    rdfs:range football:Joueur .

football:PlaysAgainst rdf:Property ;
    rdfs:label "une équipe joue contre une autre équipe" ;
    rdfs:domain football:Equipe ;
    rdfs:range football:Joueur .
